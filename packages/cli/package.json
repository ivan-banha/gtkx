{
    "name": "@gtkx/cli",
    "version": "0.4.1",
    "description": "CLI for GTKX - create and develop GTK4 React applications",
    "keywords": [
        "gtk",
        "react",
        "native",
        "linux",
        "cli",
        "scaffold",
        "hmr",
        "vite"
    ],
    "homepage": "https://eugeniodepalo.github.io/gtkx",
    "bugs": {
        "url": "https://github.com/eugeniodepalo/gtkx/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eugeniodepalo/gtkx.git",
        "directory": "packages/cli"
    },
    "license": "MPL-2.0",
    "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./dev-server": {
            "types": "./dist/dev-server.d.ts",
            "default": "./dist/dev-server.js"
        }
    },
    "bin": {
        "gtkx": "dist/cli.js"
    },
    "files": [
        "dist",
        "templates"
    ],
    "scripts": {
        "build": "tsc -b",
        "test": "vitest run"
    },
    "dependencies": {
        "@clack/prompts": "0.11.0",
        "@gtkx/ffi": "workspace:*",
        "@gtkx/react": "workspace:*",
        "@vitejs/plugin-react": "5.1.2",
        "citty": "0.1.6",
        "vite": "7.2.7"
    },
    "peerDependencies": {
        "react": "^19"
    }
}
