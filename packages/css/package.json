{
    "name": "@gtkx/css",
    "version": "0.4.1",
    "description": "Emotion-style CSS-in-JS for GTKX applications",
    "keywords": [
        "gtk",
        "gtk4",
        "css",
        "emotion",
        "css-in-js",
        "styling"
    ],
    "homepage": "https://eugeniodepalo.github.io/gtkx",
    "bugs": {
        "url": "https://github.com/eugeniodepalo/gtkx/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eugeniodepalo/gtkx.git",
        "directory": "packages/css"
    },
    "license": "MPL-2.0",
    "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b && cp ../../README.md .",
        "test": "if [ -n \"$CI\" ]; then vitest run; else GDK_BACKEND=x11 xvfb-run -a vitest run; fi"
    },
    "dependencies": {
        "@emotion/cache": "11.14.0",
        "@emotion/serialize": "1.3.3",
        "@gtkx/ffi": "workspace:*"
    }
}
